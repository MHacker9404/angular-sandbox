{
    "sourceRoot": "apps/child-parent-svc-e2e/src",
    "projectType": "application",
    "targets": {
        "e2e": {
            "executor": "@nrwl/cypress:cypress",
            "options": {
                "cypressConfig": "apps/child-parent-svc-e2e/cypress.json",
                "devServerTarget": "child-parent-svc:serve:development"
            },
            "configurations": {
                "production": {
                    "devServerTarget": "child-parent-svc:serve:production"
                }
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["apps/child-parent-svc-e2e/**/*.{js,ts}"]
            }
        }
    },
    "tags": [],
    "implicitDependencies": ["child-parent-svc"]
}
