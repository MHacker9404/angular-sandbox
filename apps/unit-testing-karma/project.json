{
    "projectType": "application",
    "sourceRoot": "apps/unit-testing-karma/src",
    "prefix": "nx-apps",
    "targets": {
        "build": {
            "executor": "@nrwl/angular:webpack-browser",
            "outputs": ["{options.outputPath}"],
            "options": {
                "outputPath": "dist/apps/unit-testing-karma",
                "index": "apps/unit-testing-karma/src/index.html",
                "main": "apps/unit-testing-karma/src/main.ts",
                "polyfills": "apps/unit-testing-karma/src/polyfills.ts",
                "tsConfig": "apps/unit-testing-karma/tsconfig.app.json",
                "inlineStyleLanguage": "scss",
                "assets": ["apps/unit-testing-karma/src/favicon.ico", "apps/unit-testing-karma/src/assets"],
                "styles": ["apps/unit-testing-karma/src/styles.scss"],
                "scripts": []
            },
            "configurations": {
                "production": {
                    "budgets": [
                        {
                            "type": "initial",
                            "maximumWarning": "500kb",
                            "maximumError": "1mb"
                        },
                        {
                            "type": "anyComponentStyle",
                            "maximumWarning": "2kb",
                            "maximumError": "4kb"
                        }
                    ],
                    "fileReplacements": [
                        {
                            "replace": "apps/unit-testing-karma/src/environments/environment.ts",
                            "with": "apps/unit-testing-karma/src/environments/environment.prod.ts"
                        }
                    ],
                    "outputHashing": "all"
                },
                "development": {
                    "buildOptimizer": false,
                    "optimization": false,
                    "vendorChunk": true,
                    "extractLicenses": false,
                    "sourceMap": true,
                    "namedChunks": true
                }
            },
            "defaultConfiguration": "development"
        },
        "serve": {
            "executor": "@nrwl/angular:webpack-server",
            "configurations": {
                "production": {
                    "browserTarget": "unit-testing-karma:build:production"
                },
                "development": {
                    "browserTarget": "unit-testing-karma:build:development",
                    "watch": true,
                    "liveReload": true
                }
            },
            "defaultConfiguration": "development",
            "options": {
                "browserTarget": "unit-testing-karma:build:development",
                "watch": true,
                "liveReload": true,
                "host": "0.0.0.0",
                "port": 4200
            }
        },
        "extract-i18n": {
            "executor": "@angular-devkit/build-angular:extract-i18n",
            "options": {
                "browserTarget": "unit-testing-karma:build"
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "options": {
                "lintFilePatterns": ["apps/unit-testing-karma/src/**/*.ts", "apps/unit-testing-karma/src/**/*.html"]
            }
        },
        "test": {
            "executor": "@angular-devkit/build-angular:karma",
            "options": {
                "main": "apps/unit-testing-karma/src/test.ts",
                "tsConfig": "apps/unit-testing-karma/tsconfig.spec.json",
                "karmaConfig": "apps/unit-testing-karma/karma.conf.js",
                "polyfills": "apps/unit-testing-karma/src/polyfills.ts",
                "styles": [],
                "scripts": [],
                "assets": []
            }
        }
    },
    "tags": []
}
