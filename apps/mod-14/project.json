{
  "root": "apps/mod-14",
  "sourceRoot": "apps/mod-14/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/web:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "development",
      "options": {
        "compiler": "swc",
        "outputPath": "dist/apps/mod-14",
        "index": "apps/mod-14/src/index.html",
        "baseHref": "/",
        "main": "apps/mod-14/src/main.tsx",
        "polyfills": "apps/mod-14/src/polyfills.ts",
        "tsConfig": "apps/mod-14/tsconfig.app.json",
        "assets": ["apps/mod-14/src/favicon.ico", "apps/mod-14/src/assets"],
        "styles": [],
        "scripts": [],
        "webpackConfig": "apps/mod-14/webpack.config.js"
      },
      "configurations": {
        "development": {
          "optimization": false,
          "outputHashing": "all",
          "sourceMap": true,
          "namedChunks": false,
          "extractLicenses": false,
          "vendorChunk": false
        },
        "production": {
          "webpackConfig": "apps/mod-14/webpack.prod.config.js",
          "fileReplacements": [
            {
              "replace": "apps/mod-14/src/environments/environment.ts",
              "with": "apps/mod-14/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      }
    },

    "serve": {
      "executor": "@nrwl/web:dev-server",
      "options": {
        "buildTarget": "mod-14:build:development",
        // https://stackoverflow.com/questions/48977194/docker-react-does-not-work-in-localhost/48977836#48977836
        "host": "0.0.0.0",
        "port": 3000,
        "hmr": true,
        "watch": true,
        "historyApiFallback": {
          "index": "./index.html"
        }
      }
    },

    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/mod-14/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": []
}
