{
    "projectType": "application",
    "root": "apps/mod04",
    "sourceRoot": "apps/mod04/src",
    "prefix": "nx-apps",
    "targets": {
        "build": {
            "executor": "@nrwl/angular:webpack-browser",
            "outputs": ["{options.outputPath}"],
            "options": {
                "outputPath": "dist/apps/mod04",
                "index": "apps/mod04/src/index.html",
                "main": "apps/mod04/src/main.ts",
                "polyfills": "apps/mod04/src/polyfills.ts",
                "tsConfig": "apps/mod04/tsconfig.app.json",
                "inlineStyleLanguage": "scss",
                "assets": [
                    "apps/mod04/src/favicon.ico",
                    "apps/mod04/src/assets"
                ],
                "styles": ["apps/mod04/src/styles.scss"],
                "scripts": []
            },
            "configurations": {
                "production": {
                    "budgets": [
                        {
                            "type": "initial",
                            "maximumWarning": "500kb",
                            "maximumError": "1mb"
                        },
                        {
                            "type": "anyComponentStyle",
                            "maximumWarning": "2kb",
                            "maximumError": "4kb"
                        }
                    ],
                    "fileReplacements": [
                        {
                            "replace": "apps/mod04/src/environments/environment.ts",
                            "with": "apps/mod04/src/environments/environment.prod.ts"
                        }
                    ],
                    "outputHashing": "all"
                },
                "development": {
                    "buildOptimizer": false,
                    "optimization": false,
                    "vendorChunk": true,
                    "extractLicenses": false,
                    "sourceMap": true,
                    "namedChunks": true
                }
            },
            "defaultConfiguration": "production"
        },
        "serve": {
            "executor": "@nrwl/angular:webpack-server",
            "configurations": {
                "production": {
                    "browserTarget": "mod04:build:production"
                },
                "development": {
                    "browserTarget": "mod04:build:development",
                    "watch": true,
                    "liveReload": true,
                    "disableHostCheck": true
                }
            },
            "defaultConfiguration": "development",
            "options": {
                "host": "0.0.0.0",
                "port": 4200
            }
        },
        "extract-i18n": {
            "executor": "@angular-devkit/build-angular:extract-i18n",
            "options": {
                "browserTarget": "mod04:build"
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "options": {
                "lintFilePatterns": [
                    "apps/mod04/src/**/*.ts",
                    "apps/mod04/src/**/*.html"
                ]
            }
        }
    },
    "tags": []
}
